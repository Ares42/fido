{
  "name": "Fido",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["injector.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["activeTab"]
}
